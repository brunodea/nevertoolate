            </intent-filter>
        </activity>

        <meta-data
            android:name="net.dean.jraw.android.REDDIT_USERNAME"
            android:value="brunodea"
            />
        <meta-data
            android:name="net.dean.jraw.android.CLIENT_ID"
            android:value="x7JYIvlBrQUlEw"
            />
        <meta-data
            android:name="net.dean.jraw.android.REDIRECT_URL"
            android:value="https://github.com/brunodea/udacity-finalproj"
            />
    </application>

</manifest>